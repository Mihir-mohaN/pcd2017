#include <stdio.h>
int main()
{ int n,b=0,c,d;
printf("write a numer....");
scanf("%d",&n);
d=n;
while(n!=0)
    {    c=n%10;
         b= b*10 + c;
        n/=10;
    }
printf("reverse number :%d",b);    
    if(d==b){
        printf("its a palindrome");
        }
         else{
            printf("its not a palindrome");
        }
       
    return 0;
    
}

    
