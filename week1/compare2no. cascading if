//comparison of two numbers cascading if

#include<stdio.h>
float comp(float a,float b){
	if(a>b){
		return a;
	}
	else if(a<b){
		return b;
	}
	else{
		return a;
	}
}

int main(){
	float a,b;
	printf("enter two numbers:");
	scanf("%f%f",&a,&b);
	float c=comp(a,b);
	printf("the larger number is:%f",c);
	return 0;
}
